<%= form_with(model: building, local: true) do |form| %>
  <div class="field">
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
  <div class="field">
    <%= form.label :address %><br>
    <%= form.text_field :address %>
  </div>
  <div class="field">
    <%= form.label :coordinates %><br>
    <%= form.text_field :coordinates %>
  </div>
  <br><br>
  <div class="actions" style="float:right">
    <%= form.submit %>
  </div>
<% end %>
